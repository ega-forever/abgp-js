    "merkletreejs": "^0.2.24"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.4",
    "@types/node": "*",
    "axios": "^0.19.2",
    "axon": "2.0.x",
    "bluebird": "^3.5.3",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "chai": "~3.5.0",
    "express": "^4.17.1",
    "mocha": "^5.2.0",
    "ts-loader": "^5.4.5",
    "ts-node": "^8.4.1",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1"
  }
}
